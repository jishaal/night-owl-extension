{
	"compilerOptions": {
		"outDir": "./build/",
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"noImplicitAny": false,
		"module": "es6",
		"moduleResolution": "node",
		"target": "es5",
		"allowJs": true,
		"jsx": "react",
		"allowSyntheticDefaultImports": true
	},
	"include": ["./src/**/*", "src/global.d.ts"]
}
